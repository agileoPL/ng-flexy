{
  "name": "@ng-flexy/demo",
  "version": "9.1.1",
  "description": "Flexy components and tools to build Angular 8+ applications",
  "keywords": [
    "Angular",
    "Library",
    "Components",
    "UI",
    "Typescript",
    "MIT"
  ],
  "license": "MIT",
  "author": {
    "name": "Miroslaw Kapinos",
    "email": "miroslaw.kapinos@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agileoPL/ng-flexy.git"
  },
  "bugs": {
    "url": "https://github.com/agileoPL/ng-flexy/issues"
  },
  "homepage": "https://github.com/agileoPL/ng-flexy#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:projects": "npm i --save-optional && npm run core:build && npm i dist/core --no-optional && npm run freezer:build && npm i dist/freezer --no-optional && npm run skins:build && npm i dist/skins --no-optional && npm run toasts:build && npm i dist/toasts --no-optional && npm run json-impexp:build && npm i dist/json-impexp --no-optional && npm run layout:build && npm i dist/layout --no-optional && npm run form:build && npm i dist/form --no-optional && npm run form-bootstrap:build && npm i dist/form-bootstrap --no-optional && npm run crud:build && npm i dist/crud --no-optional && npm run highcharts:build && npm i dist/highcharts --no-optional && npm run graphs:build && npm i dist/graphs --no-optional && npm i",
    "test:projects": "npm run toasts:test && ng test layout && ng test form",
    "demo": "ng serve demo --port 4201",
    "demo:build": "ng build demo --prod",
    "core:build": "ng build core",
    "core:watch": "ng build core --watch",
    "form:build": "ng build form",
    "form:watch": "ng build form --watch",
    "form:test": "ng test form",
    "form-bootstrap:build": "ng build form-bootstrap && cpx \"projects/form-bootstrap/src/lib/*.scss\" \"dist/form-bootstrap\"",
    "form-bootstrap:watch": "ng build form-bootstrap --watch",
    "form-bootstrap-scss:watch": "cpx \"projects/form-bootstrap/src/lib/*.scss\" \"dist/form-bootstrap\" --watch",
    "crud:build": "ng build crud && cpx \"projects/crud/src/lib/*.scss\" \"dist/crud\"",
    "crud:watch": "ng build crud --watch",
    "crud-scss:watch": "cpx \"projects/crud/src/lib/*.scss\" \"dist/crud\" --watch",
    "layout:build": "ng build layout",
    "layout:watch": "ng build layout --watch",
    "layout:test": "ng test layout",
    "freezer:watch": "ng build freezer --watch",
    "freezer:build": "ng build freezer && cpx \"projects/freezer/src/lib/freezer.scss\" \"dist/freezer\"",
    "freezer:test": "ng test freezer",
    "skins:build": "ng build skins && npm run skins:copy-scripts",
    "skins:watch": "ng build skins --watch",
    "skins:watch-scripts": "cpx \"projects/skins/src/scripts/**/*\" \"dist/skins/scripts\" --watch",
    "skins:copy-scripts": "cpx \"projects/skins/src/scripts/**/*\" \"dist/skins/scripts\"",
    "toasts:watch": "ng build toasts --watch",
    "toasts:build": "ng build toasts && cpx \"projects/toasts/src/lib/toasts.module.scss\" \"dist/toasts\"",
    "toasts:test": "ng test toasts",
    "json-impexp:build": "ng build json-impexp && cpx \"projects/json-impexp/src/lib/json-impexp.scss\" \"dist/json-impexp\"",
    "json-impexp:watch": "ng build json-impexp --watch",
    "highcharts:build": "ng build highcharts && cpx \"projects/highcharts/src/lib/highcharts.scss\" \"dist/highcharts\"",
    "highcharts:watch": "ng build highcharts --watch",
    "graphs:build": "ng build graphs && cpx \"projects/graphs/src/lib/graphs.module.scss\" \"dist/graphs\"",
    "graphs:watch": "ng build graphs --watch",
    "postinstall": "ngcc",
    "distr-build": "npm run demo:build",
    "distr-coverage": "ng test toasts --watch=false --browsers=ChromeHeadless --code-coverage && ng test form --watch=false --browsers=ChromeHeadless --code-coverage",
    "distr-copy-coverage": "cpx \"coverage/**/*\" \"dist/demo/coverage\"",
    "distr": "npm run distr-build && npm run distr-coverage && npm run distr-copy-coverage && firebase deploy"
  },
  "dependencies": {
    "@angular/animations": "~10.0.10",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.10",
    "@angular/compiler": "~10.0.10",
    "@angular/core": "~10.0.10",
    "@angular/forms": "~10.0.10",
    "@angular/platform-browser": "~10.0.10",
    "@angular/platform-browser-dynamic": "~10.0.10",
    "@angular/router": "~10.0.10",
    "@ng-select/ng-select": "^5.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.3.1",
    "font-awesome": "^4.7.0",
    "google-code-prettify": "^1.0.5",
    "highcharts": "^8.2.0",
    "jsonata": "^1.7.0",
    "jszip": "^3.2.2",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^6.0.0",
    "ngx-chips": "^2.1.0",
    "ngx-nvd3": "^1.0.9",
    "rxjs": "~6.5.4",
    "stream": "0.0.2",
    "tinycolor2": "^1.4.1",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.10",
    "@angular/language-service": "~10.0.10",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.9.7",
    "@angular-devkit/build-ng-packagr": "~0.1000.6",
    "cpx": "^1.5.0",
    "husky": "^1.3.1",
    "ng-packagr": "^10.0.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "lint-staged": "^8.2.1"
  },
  "optionalDependencies": {
    "@ng-flexy/core": "file:dist/core",
    "@ng-flexy/crud": "file:dist/crud",
    "@ng-flexy/form": "file:dist/form",
    "@ng-flexy/form-bootstrap": "file:dist/form-bootstrap",
    "@ng-flexy/freezer": "file:dist/freezer",
    "@ng-flexy/graphs": "file:dist/graphs",
    "@ng-flexy/highcharts": "file:dist/highcharts",
    "@ng-flexy/json-impexp": "file:dist/json-impexp",
    "@ng-flexy/layout": "file:dist/layout",
    "@ng-flexy/skins": "file:dist/skins",
    "@ng-flexy/toasts": "file:dist/toasts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "pretty-quick --staged"
    ]
  }
}
