{
  "name": "@ng-flexy/demo",
  "version": "12.0.1-alpha.0",
  "description": "Flexy components and tools to build Angular 8+ applications",
  "keywords": [
    "Angular",
    "Library",
    "Components",
    "UI",
    "Typescript",
    "MIT"
  ],
  "license": "MIT",
  "author": {
    "name": "Miroslaw Kapinos",
    "email": "miroslaw.kapinos@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agileoPL/ng-flexy.git"
  },
  "bugs": {
    "url": "https://github.com/agileoPL/ng-flexy/issues"
  },
  "homepage": "https://github.com/agileoPL/ng-flexy#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:projects": "npm i --save-optional && npm run core:build && npm i dist/core --no-optional && npm run freezer:build && npm i dist/freezer --no-optional && npm run skins:build && npm i dist/skins --no-optional && npm run toasts:build && npm i dist/toasts --no-optional && npm run json-impexp:build && npm i dist/json-impexp --no-optional && npm run layout:build && npm i dist/layout --no-optional && npm run form:build && npm i dist/form --no-optional && npm i",
    "test:projects": "npm run toasts:test && ng test layout && ng test form",
    "demo": "ng serve demo --port 4201",
    "demo:build": "ng build demo --configuration production",
    "core:build": "ng build core --configuration production",
    "core:watch": "ng build core --watch",
    "form:build": "ng build form --configuration production",
    "form:watch": "ng build form --watch",
    "form:test": "ng test form --code-coverage",
    "layout:build": "ng build layout --configuration production",
    "layout:watch": "ng build layout --watch",
    "layout:test": "ng test layout",
    "freezer:watch": "ng build freezer --watch",
    "freezer:build": "ng build freezer --configuration production && cpx \"projects/freezer/src/lib/freezer.scss\" \"dist/freezer\"",
    "freezer:test": "ng test freezer",
    "skins:build": "ng build skins --configuration production && npm run skins:copy-scripts",
    "skins:watch": "ng build skins --watch",
    "skins:watch-scripts": "cpx \"projects/skins/src/scripts/**/*\" \"dist/skins/scripts\" --watch",
    "skins:copy-scripts": "cpx \"projects/skins/src/scripts/**/*\" \"dist/skins/scripts\"",
    "toasts:watch": "ng build toasts --watch",
    "toasts:build": "ng build toasts --configuration production && cpx \"projects/toasts/src/lib/toasts.module.scss\" \"dist/toasts\"",
    "toasts:test": "ng test toasts",
    "json-impexp:build": "ng build json-impexp --configuration production && cpx \"projects/json-impexp/src/lib/json-impexp.scss\" \"dist/json-impexp\"",
    "json-impexp:watch": "ng build json-impexp --watch",
    "postinstall": "ngcc",
    "distr-build": "npm run demo:build",
    "distr-coverage": "ng test toasts --watch=false --browsers=ChromeHeadless --code-coverage && ng test form --watch=false --browsers=ChromeHeadless --code-coverage && ng test form-bootstrap --watch=false --browsers=ChromeHeadless --code-coverage",
    "distr-copy-coverage": "cpx \"coverage/**/*\" \"dist/demo/coverage\"",
    "distr": "npm run distr-build && npm run distr-coverage && npm run distr-copy-coverage && firebase deploy"
  },
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap": "^4.6.1",
    "font-awesome": "^4.7.0",
    "google-code-prettify": "^1.0.5",
    "jsonata": "^1.8.5",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "primeicons": "^5.0.0",
    "primeng": "^12.2.3",
    "rxjs": "~6.5.4",
    "stream": "0.0.2",
    "tinycolor2": "^1.4.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-devkit/core": "^15.2.6",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/language-service": "^15.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.37",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "^1.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^8.2.1",
    "ng-packagr": "^15.2.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.9.5"
  },
  "optionalDependencies": {
    "@ng-flexy/core": "file:dist/core",
    "@ng-flexy/form": "file:dist/form",
    "@ng-flexy/freezer": "file:dist/freezer",
    "@ng-flexy/graphs": "file:dist/graphs",
    "@ng-flexy/highcharts": "file:dist/highcharts",
    "@ng-flexy/json-impexp": "file:dist/json-impexp",
    "@ng-flexy/layout": "file:dist/layout",
    "@ng-flexy/skins": "file:dist/skins",
    "@ng-flexy/toasts": "file:dist/toasts"
  },
  "Xhusky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "Xlint-staged": {
    "*.ts": [
      "pretty-quick --staged"
    ]
  }
}